<mvc:View
    controllerName="zui50005.controller.mainView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:columnmenu="sap.m.table.columnmenu"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:table="sap.ui.table"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns:sap.ui.fl="sap.ui.fl"
    xmlns:sap.ui.dt="sap.ui.dt"
    xmlns:vm="sap.ui.fl.variants"
>
    <Page
        id="page"
        showHeader="false"
    >
        <content>
            <f:Form
                id="_IDGenForm1"
                editable="true"
            >
                <f:layout>
                    <f:ResponsiveGridLayout
                        id="_IDGenResponsiveGridLayout"
                        labelSpanL="1"
                        labelSpanM="1"
                        emptySpanL="0"
                        emptySpanM="0"
                        columnsL="1"
                        columnsM="1"
                    />
                </f:layout>
                <f:formContainers>
                    <f:FormContainer id="_IDGenFormContainer">
                        <f:formElements>
                            <f:FormElement id="_IDGenFormElement">
                                <f:fields>
                                    <l:HorizontalLayout
                                        id="_IDGenHorizontalLayout"
                                    >
                                        <Label
                                            id="_IDGenLabel"
                                            labelFor='ids1'
                                            text="工厂："
                                            class="myMasterHead"
                                        />
                                        <Input
                                            id='ids1'
                                            width="8rem"
                                            value="{searchModel>/productionplant}"
                                            editable="false"
                                        />
                                        <Label
                                            id="_IDGenLabel1"
                                            labelFor='MultiInput1'
                                            text="采购订单："
                                            class="myMasterHead"
                                        />
                                        <MultiInput
                                            id="MultiInput1"
                                            width="36rem"
                                            showValueHelp="false"
                                            showSuggestion="false"
                                            tokens="{
                                                path: 'searchModel>/purchaseorderList',
                                                sorter: { path: 'key' }
                                            }"
                                        >
                                            <tokens>
                                                <Token
                                                    id="_IDGenToken"
                                                    key="{searchModel>key}"
                                                    text="{searchModel>text}"
                                                />
                                            </tokens>
                                        </MultiInput>
                                    </l:HorizontalLayout>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement id="_IDGenFormElement1">
                                <f:fields>
                                    <l:HorizontalLayout
                                        id="_IDGenHorizontalLayout1"
                                    >
                                        <Label
                                            id="_IDGenLabel3"
                                            labelFor='ids4'
                                            text="产品名称："
                                            class="myMasterHead"
                                        />
                                        <Input
                                            id='ids4'
                                            width="8rem"
                                            value="{searchModel>/cpname}"
                                        />
                                        <Label
                                            id="_IDGenLabel4"
                                            labelFor='ids5'
                                            text="开始日期："
                                            class="myMasterHead"
                                        />
                                        <DatePicker
                                            id='ids5'
                                            width="8rem"
                                            value="{
											path: 'searchModel>/purchaseorderdatestart',
											type: 'sap.ui.model.type.Date',
											formatOptions: {
                                                style: 'short',
												source: {pattern: 'yyyyMMdd'}
											}}"
                                        />
                                        <Label
                                            id="_IDGenLabel5"
                                            labelFor='ids6'
                                            text="结束日期："
                                            class="myMasterHead"
                                        />
                                        <DatePicker
                                            id='ids6'
                                            width="8rem"
                                            value="{
											path: 'searchModel>/purchaseorderdateend',
											type: 'sap.ui.model.type.Date',
											formatOptions: {
                                                style: 'short',
												source: {pattern: 'yyyyMMdd'}
											}}"
                                        />
                                        <Button
                                            id="_IDGenButtonre"
                                            text="重置"
                                            icon="sap-icon://search"
                                            press=".onReset"
                                            type="Accept"
                                            class="myMasterHead"
                                        />
                                        <Button
                                            id="_IDGenButton"
                                            text="查询"
                                            icon="sap-icon://search"
                                            press=".onDataSearch"
                                            type="Accept"
                                            class="myMasterHead"
                                        />
                                    </l:HorizontalLayout>
                                </f:fields>
                            </f:FormElement>
                        </f:formElements>
                    </f:FormContainer>
                </f:formContainers>
            </f:Form>
        </content>
        <content>
            <table:Table
                id="_IDGenTable"
                rows="{dataModel>/data}"
                rowMode="Auto"
                selectionMode="MultiToggle"
                paste="onPaste"
                ariaLabelledBy="title"
                class="myFontSize"
                sort="onSort"
                columnResize="onColumnResize"
                columnMove="onColumnMove"
                sap.ui.fl:flexibility="sap/m/flexibility/EngineFlex"
            >
                <table:extension>
                    <OverflowToolbar
                        id="_IDGenOverflowToolbar"
                        style="Clear"
                    >
                        <Title
                            id="title"
                            text="{appModel>/currentType}"
                        />
                        <ToolbarSeparator id="_IDGenToolbarSeparator" />
                        <vm:VariantManagement
                            id="p13nVm"
                            for="_IDGenTable"
                        />
                        <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                        <Button
                            id="_IDGenButton1"
                            icon="sap-icon://edit"
                            text="更改"
                            press="onEdit"
                        />
                        <Button
                            id="_IDGenButton2"
                            icon="sap-icon://save"
                            text="生成领料单"
                            press="onSave"
                            enabled="{appModel>/enableSave}"
                        />
                        <Button
                            id="_IDGenButton5"
                            icon="sap-icon://excel-attachment"
                            text="导出"
                            press="onExport"
                        />
                        <Button
                            id="_IDGenButton4"
                            icon="sap-icon://action-settings"
                            text="设置"
                            press="openPersoDialog"
                        />
                    </OverflowToolbar>
                </table:extension>
                <table:dependents>
                    <columnmenu:Menu id="menu">
                        <columnmenu:items>
                            <columnmenu:ActionItem
                                id="_IDGenActionItem1"
                                icon="sap-icon://sort"
                                label="Sort"
                                press="onColumnHeaderItemPress"
                            />
                            <columnmenu:ActionItem
                                id="_IDGenActionItem2"
                                icon="sap-icon://table-column"
                                label="Columns"
                                press="onColumnHeaderItemPress"
                            />
                        </columnmenu:items>
                    </columnmenu:Menu>
                </table:dependents>
                <table:noData>
                    <IllustratedMessage
                        id="_IDGenIllustratedMessage"
                        enableVerticalResponsiveness="true"
                    />
                </table:noData>
                <table:rowSettingsTemplate>
                    <table:RowSettings
                        id="_IDGenRowSettings"
                        highlight="{dataModel>flagIcon}"
                        highlightText="{dataModel>yy1_msg}"
                    />
                </table:rowSettingsTemplate>
                <table:columns>
                    <table:Column
                        id="_IGenColumn"
                        width="4rem"
                        app:p13nKey="yy1_msg_col"
                        sortProperty="yy1_msg"
                        filterProperty="yy1_msg"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IGenLabel7"
                            text="消息"
                        />
                        <table:template>
                            <Text
                                id="_IGenText"
                                text="{dataModel>yy1_msg}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn312"
                        width="6rem"
                        app:p13nKey="zllno_col"
                        sortProperty="zllno"
                        filterProperty="zllno"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel321"
                            text="领料单号"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText312"
                                text="{dataModel>zllno}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn"
                        width="4rem"
                        app:p13nKey="productionplant_col"
                        sortProperty="productionplant"
                        filterProperty="productionplant"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel7"
                            text="工厂"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText"
                                text="{dataModel>productionplant}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn1"
                        width="6rem"
                        app:p13nKey="purchaseorder_col"
                        sortProperty="purchaseorder"
                        filterProperty="purchaseorder"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel8"
                            text="采购订单"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText1"
                                text="{dataModel>purchaseorder}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn1333"
                        width="6rem"
                        app:p13nKey="purchaseorderitem_col"
                        sortProperty="purchaseorderitem"
                        filterProperty="purchaseorderitem"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel8333"
                            text="采购行"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText1333"
                                text="{dataModel>purchaseorderitem}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn8"
                        width="6rem"
                        app:p13nKey="cp_col"
                        sortProperty="cp"
                        filterProperty="cp"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel15"
                            text="产品编码"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText8"
                                text="{dataModel>cp}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn15"
                        width="12rem"
                        app:p13nKey="cpname_col"
                        sortProperty="cpname"
                        filterProperty="cpname"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel22"
                            text="产品名称"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText15"
                                text="{dataModel>cpname}"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn10"
                        width="5rem"
                        app:p13nKey="reservationitem_col"
                        sortProperty="reservationitem"
                        filterProperty="reservationitem"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel17"
                            text="预留项目"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText152"
                                text="{dataModel>reservationitem}"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn51"
                        width="6rem"
                        app:p13nKey="zj_col"
                        sortProperty="zj"
                        filterProperty="zj"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel51"
                            text="组件物料"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText51"
                                text="{dataModel>zj}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn52"
                        width="10rem"
                        app:p13nKey="zjname_col"
                        sortProperty="zjname"
                        filterProperty="zjname"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel52"
                            text="组件描述"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText52"
                                text="{dataModel>zjname}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn521"
                        width="8rem"
                        app:p13nKey="zjgroupname_col"
                        sortProperty="zjgroupname"
                        filterProperty="zjgroupname"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel521"
                            text="物料类型"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText521"
                                text="{dataModel>zjgroupname}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn53"
                        width="6rem"
                        app:p13nKey="zjqty_col"
                        sortProperty="zjqty"
                        filterProperty="zjqty"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel53"
                            text="需求数量"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText53"
                                text="{dataModel>zjqty}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn54"
                        width="4rem"
                        app:p13nKey="zjunit_col"
                        sortProperty="zjunit"
                        filterProperty="zjunit"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel54"
                            text="单位"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText253"
                                text="{dataModel>zjunit}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn55"
                        width="6rem"
                        app:p13nKey="requestedqty_col"
                        sortProperty="requestedqty"
                        filterProperty="requestedqty"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel55"
                            text="申请数量"
                        />
                        <table:template>
                            <Input
                                id="_IDGenText55"
                                type="Number"
                                value="{dataModel>requestedqty}"
                                editable="{dataModel>editable}"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn5633"
                        width="8rem"
                        app:p13nKey="subcontractor_col"
                        sortProperty="subcontractor"
                        filterProperty="subcontractor"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel5633"
                            text="供应商"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText5633"
                                text="{dataModel>subcontractor}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn563"
                        width="8rem"
                        app:p13nKey="zcreate_date_col"
                        sortProperty="zcreate_date"
                        filterProperty="zcreate_date"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel563"
                            text="创建日期"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText563"
                                text="{dataModel>zcreate_date}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                </table:columns>
            </table:Table>
        </content>
    </Page>
</mvc:View>
