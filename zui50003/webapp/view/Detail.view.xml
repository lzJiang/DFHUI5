<mvc:View
    displayBlock="true"
    controllerName="zui50003.controller.Detail"
    height="100%"
    xmlns="sap.uxap"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:m="sap.m"
    xmlns:f="sap.f"
    xmlns:form="sap.ui.layout.form"
>
    <ObjectPageLayout
        id="ObjectPageLayout"
        showTitleInHeaderContent="true"
        alwaysShowContentHeader="false"
        preserveHeaderStateOnScroll="false"
        headerContentPinnable="true"
        isChildPage="true"
        upperCaseAnchorBar="false"
    >
        <headerTitle>
            <ObjectPageDynamicHeaderTitle
                id="_IDGenObjectPageDynamicHeaderTitle"
            >
                <expandedHeading>
                    <m:Title
                        id="_IDGenTitle"
                        text="{dataModel>pk_no}"
                        wrapping="true"
                        class="sapUiSmallMarginEnd"
                    />
                </expandedHeading>

                <snappedHeading>
                    <m:FlexBox
                        id="_IDGenFlexBox"
                        wrap="Wrap"
                        fitContainer="true"
                        alignItems="Center"
                    >
                        <m:FlexBox
                            id="_IDGenFlexBox1"
                            wrap="NoWrap"
                            fitContainer="true"
                            alignItems="Center"
                            class="sapUiTinyMarginEnd"
                        >
                            <!-- <m:Avatar src="../../../../../../../{dataModel>ProductPicUrl}" displaySize="S" displayShape="Square"/> -->
                            <m:Title
                                id="_IDGenTitle1"
                                text="{dataModel>pk_no}"
                                wrapping="true"
                                class="sapUiTinyMarginEnd"
                            />
                        </m:FlexBox>
                    </m:FlexBox>
                </snappedHeading>

                <navigationActions>
                    <m:OverflowToolbarButton
                        type="Transparent"
                        icon="sap-icon://full-screen"
                        press="handleFullScreen"
                        id="enterFullScreenBtn"
                        tooltip="Enter Full Screen Mode"
                        visible="{= ${/actionButtonsInfo/midColumn/fullScreen} !== null }"
                    />
                    <m:OverflowToolbarButton
                        type="Transparent"
                        icon="sap-icon://exit-full-screen"
                        press="handleExitFullScreen"
                        id="exitFullScreenBtn"
                        tooltip="Exit Full Screen Mode"
                        visible="{= ${/actionButtonsInfo/midColumn/exitFullScreen} !== null }"
                    />
                    <m:OverflowToolbarButton
                        id="_IDGenOverflowToolbarButton"
                        type="Transparent"
                        icon="sap-icon://decline"
                        press="handleClose"
                        tooltip="Close middle column"
                        visible="{= ${/actionButtonsInfo/midColumn/closeColumn} !== null }"
                    />
                </navigationActions>

                <!-- <actions>
							<m:Button
									text="Edit"
									type="Emphasized"
									press="toggleAreaPriority"/>
							<m:Button
									text="Delete"
									type="Transparent"/>
							<m:Button
									text="Copy"
									type="Transparent"/>
							<m:Button
									text="Toggle Footer"
									type="Transparent"
									press="onToggleFooter"/>
							<m:Button
									icon="sap-icon://action"
									type="Transparent"/>
						</actions> -->
            </ObjectPageDynamicHeaderTitle>
        </headerTitle>

        <headerContent>
            <m:FlexBox
                id="_IDGenFlexBox2"
                wrap="Wrap"
                fitContainer="true"
                alignItems="Stretch"
            >
                <!-- <m:Avatar src="../../../../../../../{dataModel>ProductPicUrl}"
								  displaySize="L"
								  displayShape="Square"
								  class="sapUiTinyMarginEnd">
						</m:Avatar> -->
                <m:VBox
                    id="_IDGenVBox11"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel11"
                        text="SAP凭证号"
                    />
                    <m:Text
                        id="_IDGenText51"
                        text="{dataModel>accountingdocument}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox1"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel"
                        text="公司代码"
                    />
                    <m:Text
                        id="_IDGenText5"
                        text="{dataModel>companycode}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox2"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel1"
                        text="凭证日期"
                    />
                    <m:Text
                        id="_IDGenText6"
                        text="{dataModel>documentdate}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox3"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel2"
                        text="过账日期"
                    />
                    <m:Text
                        id="_IDGenText7"
                        text="{dataModel>postingdate}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox4"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel3"
                        text="凭证类型"
                    />
                    <m:Text
                        id="_IDGenText8"
                        text="{dataModel>accountingdocumenttype}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox5"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel6"
                        text="抬头文本"
                    />
                    <m:Text
                        id="_IDGenText9"
                        text="{dataModel>DocumentHeaderText}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox6"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel7"
                        text="交易货币"
                    />
                    <m:Text
                        id="_IDGenText10"
                        text="{dataModel>TransactionCurrency}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox7"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel8"
                        text="借项金额"
                    />
                    <m:ObjectNumber
                        id="_IDGenObjectNumber2"
                        number="{dataModel>jxje} {dataModel>transactioncurrency}"
                        emphasized="false"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox8"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel9"
                        text="贷项金额"
                    />
                    <m:ObjectNumber
                        id="_IDGenObjectNumber3"
                        number="{dataModel>dxje} {dataModel>transactioncurrency}"
                        emphasized="false"
                    />
                </m:VBox>
            </m:FlexBox>
        </headerContent>

        <sections>
            <ObjectPageSection
                id="_IDGenObjectPageSection"
                title="凭证行"
            >
                <subSections>
                    <ObjectPageSubSection id="_IDGenObjectPageSubSection">
                        <blocks>
                            <m:Table
                                id="sssssTable"
                                items="{path : 'detailModel>/item'}"
                                autoPopinMode="true"
                                contextualWidth="Auto"
                            >
                                <m:columns>
                                    <m:Column
                                        width="8rem"
                                        importance="High"
                                        id="_IDGenColumn5111"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText1111"
                                            text="SAP凭证号"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="5rem"
                                        importance="High"
                                        id="_IDGenColumn5"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText11"
                                            text="项目号"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="5rem"
                                        importance="High"
                                        id="_IDGenColumn6"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText12"
                                            text="项目类型"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="8rem"
                                        importance="High"
                                        id="_IDGenColumn7"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText13"
                                            text="总账科目"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="8rem"
                                        importance="High"
                                        id="_IDGenColumn8"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText14"
                                            text="客户编码"
                                        />
                                    </m:Column>
                                    <m:Column
                                        id="_IDGenColumn4"
                                        width="8rem"
                                        importance="High"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText3"
                                            text="供应商编码"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="8rem"
                                        importance="High"
                                        id="_IDGenColumn9"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText15"
                                            text="备选统驭科目"
                                        />
                                    </m:Column>

                                    <m:Column
                                        width="6rem"
                                        importance="High"
                                        id="_IDGenColumn10"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText16"
                                            text="经销商名称"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="5rem"
                                        importance="High"
                                        id="_IDGenColumn11"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText17"
                                            text="借贷码"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="High"
                                        id="_IDGenColumn12"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText18"
                                            text="金额(交易货币)"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="High"
                                        id="_IDGenColumn13"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText19"
                                            text="金额(集团货币)"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="Low"
                                        id="_IDGenColumn14"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText20"
                                            text="开户行"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="8rem"
                                        importance="Low"
                                        id="_IDGenColumn15"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText21"
                                            text="开户行账户"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="8rem"
                                        importance="Low"
                                        id="_IDGenColumn151"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText211"
                                            text="成本中心"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="Low"
                                        id="_IDGenColumn16"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText22"
                                            text="会员ID"
                                        />
                                    </m:Column>
                                    <m:Column
                                        id="_IDGenColumn1"
                                        width="6rem"
                                        importance="Low"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText"
                                            text="会员名称"
                                        />
                                    </m:Column>
                                    <m:Column
                                        id="_IDGenColumn2"
                                        width="6rem"
                                        importance="Low"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText1"
                                            text="经销商ID"
                                        />
                                    </m:Column>
                                    <m:Column
                                        id="_IDGenColumn3"
                                        width="6rem"
                                        importance="Low"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText2"
                                            text="费用所属年月"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="Low"
                                        id="_IDGenColumn17"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText23"
                                            text="原因代码"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="Low"
                                        id="_IDGenColumn18"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText24"
                                            text="利润中心"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="Low"
                                        id="_IDGenColumn19"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText25"
                                            text="销售订单"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        id="_IDGenColumn20"
                                        hAlign="Center"
                                        importance="Low"
                                    >
                                        <m:Text
                                            id="_IDGenText26"
                                            text="销售订单项目"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        id="_IDGenColumn21"
                                        hAlign="Center"
                                        importance="Low"
                                    >
                                        <m:Text
                                            id="_IDGenText27"
                                            text="销售组织"
                                        />
                                    </m:Column>
                                </m:columns>
                                <m:items>
                                    <m:ColumnListItem
                                        id="_IDGenColumnListItem1"
                                    >
                                        <m:cells>
                                           <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier33"
                                                text="{detailModel>accountingdocument}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier3"
                                                text="{detailModel>pk_line_no_1}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier4"
                                                text="{detailModel>koart}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier5"
                                                text="{detailModel>glaccount}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier6"
                                                text="{detailModel>customer}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier"
                                                text="{detailModel>vendor}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier7"
                                                text="{detailModel>altvrecnclnaccts}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier8"
                                                text="{detailModel>documentitemtext}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier9"
                                                text="{detailModel>debitcreditcode}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier10"
                                                text="{detailModel>amountintransactioncurrency}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier11"
                                                text="{detailModel>amountingroupcurrency}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier12"
                                                text="{detailModel>housebank}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier13"
                                                text="{detailModel>housebankaccount}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier131"
                                                text="{detailModel>costcenter}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier14"
                                                text="{detailModel>assignmentreference}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier146"
                                                text="{detailModel>reference3idbybusinesspartner}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier144"
                                                text="{detailModel>reference1idbybusinesspartner}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier145"
                                                text="{detailModel>reference2idbybusinesspartner}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier15"
                                                text="{detailModel>reasoncode}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier16"
                                                text="{detailModel>profitcenter}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier17"
                                                text="{detailModel>salesdocument}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier19"
                                                text="{detailModel>salesdocumentitem}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier20"
                                                text="{detailModel>salesorganization}"
                                            />
                                        </m:cells>
                                    </m:ColumnListItem>
                                </m:items>
                            </m:Table>
                        </blocks>
                    </ObjectPageSubSection>
                </subSections>
            </ObjectPageSection>
        </sections>
    </ObjectPageLayout>
</mvc:View>
