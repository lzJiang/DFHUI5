<mvc:View
    controllerName="zui50009.controller.mainView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:columnmenu="sap.m.table.columnmenu"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:table="sap.ui.table"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns:sap.ui.fl="sap.ui.fl"
    xmlns:sap.ui.dt="sap.ui.dt"
    xmlns:vm="sap.ui.fl.variants"
>
    <Page
        id="page"
        showHeader="false"
    >
        <content>
            <f:Form
                id="_IDGenForm1"
                editable="true"
            >
                <f:layout>
                    <f:ResponsiveGridLayout
                        id="_IDGenResponsiveGridLayout"
                        labelSpanL="1"
                        labelSpanM="1"
                        emptySpanL="0"
                        emptySpanM="0"
                        columnsL="1"
                        columnsM="1"
                    />
                </f:layout>
                <f:formContainers>
                    <f:FormContainer id="_IDGenFormContainer">
                        <f:formElements>
                            <f:FormElement id="_IDGenFormElement">
                                <f:fields>
                                    <l:HorizontalLayout
                                        id="_IDGenHorizontalLayout"
                                    >
                                        <Label
                                            id="_IDGenLabel"
                                            labelFor='ids1'
                                            text="工厂："
                                            class="myMasterHead"
                                        />
                                        <Input
                                            id='ids1'
                                            width="8rem"
                                            value="{searchModel>/productionplant}"
                                            editable="false"
                                        />
                                        <Label
                                            id="_IDGenLabel1"
                                            labelFor='_IDGenSelectSearch1'
                                            text="领料类型："
                                            class="myMasterHead"
                                        />
                                        <Select
                                            id="_IDGenSelectSearch1"
                                            width="8rem"
                                            forceSelection="false"
                                            selectedKey="{searchModel>/zlllx}"
                                        >
                                            <core:Item
                                                id="_IDGenItem"
                                                key="01"
                                                text="生产领料"
                                            />
                                            <core:Item
                                                id="_IDGenItem1"
                                                key="02"
                                                text="委外领料"
                                            />
                                            <core:Item
                                                id="_IDGenItem7"
                                                key="03"
                                                text="其他领料"
                                            />
                                            <core:Item
                                                id="_IDGenItem8"
                                                key="04"
                                                text="其他退料"
                                            />
                                        </Select>
                                        <Label
                                            id="_IDGenLabel2"
                                            labelFor='_IDGenSelectSearch2'
                                            text="单据状态："
                                            class="myMasterHead"
                                        />
                                        <Select
                                            id="_IDGenSelectSearch2"
                                            width="8rem"
                                            forceSelection="false"
                                            selectedKey="{searchModel>/zllzt}"
                                        >
                                            <core:Item
                                                id="_IDGenItem9"
                                                key="NEW"
                                                text="新建"
                                            />
                                            <core:Item
                                                id="_IDGenItem10"
                                                key="RELEASE"
                                                text="下发WMS"
                                            />
                                            <core:Item
                                                id="_IDGenItem11"
                                                key="DELETE"
                                                text="删除"
                                            />
                                        </Select>
                                        <Label
                                            id="_IDGenLabel4"
                                            labelFor='ids5'
                                            text="开始日期："
                                            class="myMasterHead"
                                        />
                                        <DatePicker
                                            id='ids5'
                                            width="8rem"
                                            value="{
											path: 'searchModel>/zcreate_datestart',
											type: 'sap.ui.model.type.Date',
											formatOptions: {
                                                style: 'short',
												source: {pattern: 'yyyyMMdd'}
											}}"
                                        />
                                        <Label
                                            id="_IDGenLabel5"
                                            labelFor='ids6'
                                            text="结束日期："
                                            class="myMasterHead"
                                        />
                                        <DatePicker
                                            id='ids6'
                                            width="8rem"
                                            value="{
											path: 'searchModel>/zcreate_dateend',
											type: 'sap.ui.model.type.Date',
											formatOptions: {
                                                style: 'short',
												source: {pattern: 'yyyyMMdd'}
											}}"
                                        />
                                    </l:HorizontalLayout>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement id="_IDGenFormElement1">
                                <f:fields>
                                    <l:HorizontalLayout
                                        id="_IDGenHorizontalLayout1"
                                    >
                                        <Label
                                            id="_IDGenLabel6"
                                            labelFor='ids1'
                                            text="领料单号："
                                            class="myMasterHead"
                                        />
                                        <Input
                                            id="_IDGenInput1"
                                            width="8rem"
                                            value="{searchModel>/zllno}"
                                        />
                                        <Label
                                            id="_IDGenLabel9"
                                            labelFor='ids1'
                                            text="组件物料："
                                            class="myMasterHead"
                                        />
                                        <Input
                                            id="_IDGenInput2"
                                            width="8rem"
                                            value="{searchModel>/zjname}"
                                        />
                                        <Label
                                            id="_IDGenLabel3"
                                            labelFor='ids4'
                                            text="车间："
                                            class="myMasterHead"
                                        />
                                        <Input
                                            id='ids4'
                                            width="8rem"
                                            value="{searchModel>/cpname}"
                                        />
                                        <Button
                                            id="_IDGenButtonre"
                                            text="重置"
                                            icon="sap-icon://search"
                                            press=".onReset"
                                            type="Accept"
                                            class="myMasterHead"
                                        />
                                        <Button
                                            id="_IDGenButton"
                                            text="查询"
                                            icon="sap-icon://search"
                                            press=".onDataSearch"
                                            type="Accept"
                                            class="myMasterHead"
                                        />
                                    </l:HorizontalLayout>
                                </f:fields>
                            </f:FormElement>
                        </f:formElements>
                    </f:FormContainer>
                </f:formContainers>
            </f:Form>
        </content>
        <content>
            <table:Table
                id="_IDGenTable"
                rows="{dataModel>/data}"
                rowMode="Auto"
                selectionMode="MultiToggle"
                paste="onPaste"
                ariaLabelledBy="title"
                class="myFontSize"
                sort="onSort"
                columnResize="onColumnResize"
                columnMove="onColumnMove"
                sap.ui.fl:flexibility="sap/m/flexibility/EngineFlex"
            >
                <table:extension>
                    <OverflowToolbar
                        id="_IDGenOverflowToolbar"
                        style="Clear"
                    >
                        <Title
                            id="title"
                            text="{appModel>/currentType}"
                        />
                        <ToolbarSeparator id="_IDGenToolbarSeparator" />
                        <vm:VariantManagement
                            id="p13nVm"
                            for="_IDGenTable"
                        />
                        <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                        <Button
                            id="_IDGenButton1"
                            icon="sap-icon://delete"
                            text="删除单据"
                            press="onDelete"
                        />
                        <Button
                            id="_IDGenButton2"
                            icon="sap-icon://shipping-status"
                            text="下发WMS"
                            press="onSendwms"
                        />
                        <Button
                            id="_IDGenButton5"
                            icon="sap-icon://excel-attachment"
                            text="导出"
                            press="onExport"
                        />
                        <Button
                            id="_IDGenButton4"
                            icon="sap-icon://action-settings"
                            text="设置"
                            press="openPersoDialog"
                        />
                    </OverflowToolbar>
                </table:extension>
                <table:dependents>
                    <columnmenu:Menu id="menu">
                        <columnmenu:items>
                            <columnmenu:ActionItem
                                id="_IDGenActionItem1"
                                icon="sap-icon://sort"
                                label="Sort"
                                press="onColumnHeaderItemPress"
                            />
                            <columnmenu:ActionItem
                                id="_IDGenActionItem2"
                                icon="sap-icon://table-column"
                                label="Columns"
                                press="onColumnHeaderItemPress"
                            />
                        </columnmenu:items>
                    </columnmenu:Menu>
                </table:dependents>
                <table:noData>
                    <IllustratedMessage
                        id="_IDGenIllustratedMessage"
                        enableVerticalResponsiveness="true"
                    />
                </table:noData>
                <table:rowSettingsTemplate>
                    <table:RowSettings
                        id="_IDGenRowSettings"
                        highlight="{dataModel>flagIcon}"
                        highlightText="{dataModel>yy1_msg}"
                    />
                </table:rowSettingsTemplate>
                <table:columns>
                    <table:Column
                        id="_IGenColumn"
                        width="4rem"
                        app:p13nKey="yy1_msg_col"
                        sortProperty="yy1_msg"
                        filterProperty="yy1_msg"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IGenLabel7"
                            text="消息"
                        />
                        <table:template>
                            <Text
                                id="_IGenText"
                                text="{dataModel>yy1_msg}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn312"
                        width="10rem"
                        app:p13nKey="zllno_col"
                        sortProperty="zllno"
                        filterProperty="zllno"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel321"
                            text="领料单号"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText312"
                                text="{dataModel>zllno}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn"
                        width="5rem"
                        app:p13nKey="zllitemno_col"
                        sortProperty="zllitemno"
                        filterProperty="zllitemno"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel7"
                            text="行号"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText"
                                text="{dataModel>zllitemno}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn1"
                        width="6rem"
                        app:p13nKey="zlllxtext_col"
                        sortProperty="zlllxtext"
                        filterProperty="zlllxtext"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel8"
                            text="领料类型"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText1"
                                text="{dataModel>zlllxtext}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn6"
                        width="6rem"
                        app:p13nKey="zllzttext_col"
                        sortProperty="zllzttext"
                        filterProperty="zllzttext"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel13"
                            text="单据状态"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText6"
                                text="{dataModel>zllzttext}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn7"
                        width="4rem"
                        app:p13nKey="productionplant_col"
                        sortProperty="productionplant"
                        filterProperty="productionplant"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel14"
                            text="工厂"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText7"
                                text="{dataModel>productionplant}"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn51"
                        width="6rem"
                        app:p13nKey="zj_col"
                        sortProperty="zj"
                        filterProperty="zj"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel51"
                            text="组件物料"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText51"
                                text="{dataModel>zj}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn52"
                        width="10rem"
                        app:p13nKey="zjname_col"
                        sortProperty="zjname"
                        filterProperty="zjname"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel52"
                            text="组件描述"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText52"
                                text="{dataModel>zjname}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn521"
                        width="8rem"
                        app:p13nKey="zjgroupname_col"
                        sortProperty="zjgroupname"
                        filterProperty="zjgroupname"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel521"
                            text="物料类型"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText521"
                                text="{dataModel>zjgroupname}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn55"
                        width="6rem"
                        app:p13nKey="requestedqty_col"
                        sortProperty="requestedqty"
                        filterProperty="requestedqty"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel55"
                            text="申请数量"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText55"
                                text="{dataModel>requestedqty}"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn54"
                        width="4rem"
                        app:p13nKey="zjunit_col"
                        sortProperty="zjunit"
                        filterProperty="zjunit"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel54"
                            text="单位"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText253"
                                text="{dataModel>zjunit}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn561"
                        width="6rem"
                        app:p13nKey="zcjtext_col"
                        sortProperty="zcjtext"
                        filterProperty="zcjtext"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel561"
                            text="车间"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText561"
                                text="{dataModel>zcjtext}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn5612"
                        width="6rem"
                        app:p13nKey="storagelocationto_col"
                        sortProperty="storagelocationto"
                        filterProperty="storagelocationto"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel5612"
                            text="发出地点"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText5612"
                                text="{dataModel>storagelocationtoname}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn56"
                        width="6rem"
                        app:p13nKey="storagelocationname_col"
                        sortProperty="storagelocationname"
                        filterProperty="storagelocationname"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel56"
                            text="接收地点"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText5611"
                                text="{dataModel>storagelocationname}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn562"
                        width="8rem"
                        app:p13nKey="manufacturingorder_col"
                        sortProperty="manufacturingorder"
                        filterProperty="manufacturingorder"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel562"
                            text="生产订单"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText562"
                                text="{dataModel>manufacturingorder}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn5621"
                        width="8rem"
                        app:p13nKey="yy1_mfgbatch_ord_col"
                        sortProperty="yy1_mfgbatch_ord"
                        filterProperty="yy1_mfgbatch_ord"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel5621"
                            text="生产批次"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText5621"
                                text="{dataModel>yy1_mfgbatch_ord}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn2"
                        width="8rem"
                        app:p13nKey="cp_col"
                        sortProperty="cp"
                        filterProperty="cp"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel10"
                            text="产品"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText3"
                                text="{dataModel>cp}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn3"
                        width="8rem"
                        app:p13nKey="cpname_col"
                        sortProperty="cpname"
                        filterProperty="cpname"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel11"
                            text="产品描述"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText4"
                                text="{dataModel>cpname}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn5622"
                        width="8rem"
                        app:p13nKey="purchaseorder_col"
                        sortProperty="purchaseorder"
                        filterProperty="purchaseorder"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel5622"
                            text="采购订单"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText5622"
                                text="{dataModel>purchaseorder}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn5623"
                        width="6rem"
                        app:p13nKey="purchaseorderitem_col"
                        sortProperty="purchaseorderitem"
                        filterProperty="purchaseorderitem"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel5623"
                            text="采购单行"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText5623"
                                text="{dataModel>purchaseorderitem}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn5626"
                        width="8rem"
                        app:p13nKey="subcontractor_col"
                        sortProperty="subcontractor"
                        filterProperty="subcontractor"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel5626"
                            text="供应商"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText5626"
                                text="{dataModel>subcontractor}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn8"
                        width="8rem"
                        app:p13nKey="zcreate_date_col"
                        sortProperty="zcreate_date"
                        filterProperty="zcreate_date"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel15"
                            text="创建日期"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText8"
                                text="{dataModel>zcreate_date}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn5"
                        width="10rem"
                        app:p13nKey="zhlbz_col"
                        sortProperty="zhlbz"
                        filterProperty="zhlbz"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel12"
                            text="含量备注"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText2"
                                text="{dataModel>zhlbz}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn9"
                        width="10rem"
                        app:p13nKey="zbz_col"
                        sortProperty="zbz"
                        filterProperty="zbz"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel16"
                            text="备注"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText5"
                                text="{dataModel>zbz}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                </table:columns>
            </table:Table>
        </content>
    </Page>
</mvc:View>
