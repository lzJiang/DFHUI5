<mvc:View
    controllerName="zui50018.controller.mainView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:columnmenu="sap.m.table.columnmenu"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:table="sap.ui.table"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns:sap.ui.fl="sap.ui.fl"
    xmlns:sap.ui.dt="sap.ui.dt"
    xmlns:vm="sap.ui.fl.variants"
    xmlns:r="sap.ui.table.rowmodes"
    xmlns:uxap="sap.uxap"
>
    <Page
        id="page"
        showHeader="false"
    >
        <content>
            <Panel
                id="_IDGenPanel"
                expandable="true"
                expanded="true"
                headerText="选择条件"
            >
                <FlexBox
                    id="_IDGenFlexBox"
                    alignItems="Center"
                    justifyContent="SpaceAround"
                >
                    <FlexBox
                        id="_IDGenFlexBox1"
                        direction="Column"
                        alignItems="Start"
                    >
                        <Label
                            id="_IDGenLabel"
                            text="款项性质："
                            class="myMasterHead"
                        />
                        <Input
                            id="_IDGenInput1"
                            class="myMasterHead"
                            value="{searchModel>/extend1}"
                        />
                    </FlexBox>
                    <FlexBox
                        id="_IDGenFlexBox2"
                        direction="Column"
                        alignItems="Start"
                    >
                        <Label
                            id="_IDGenLabel1"
                            text="交易流水号："
                            class="myMasterHead"
                        />
                        <Input
                            id="_IDGenInput2"
                            class="myMasterHead"
                            value="{searchModel>/transactionserialnumber}"
                        />
                    </FlexBox>
                    <FlexBox
                        id="_IDGenFlexBox3"
                        direction="Column"
                        alignItems="Start"
                    >
                        <Label
                            id="_IDGenLabel2"
                            text="公司代码："
                            class="myMasterHead"
                        />
                        <Input
                            id="_IDGenInput3"
                            class="myMasterHead"
                            value="{searchModel>/companycode}"
                        />
                    </FlexBox>
                    <FlexBox
                        id="_IDGenFlexBox4"
                        direction="Column"
                        alignItems="Start"
                    >
                        <Label
                            id="_IDGenLabel3"
                            text="客商编码："
                            class="myMasterHead"
                        />
                        <Input
                            id="_IDGenInput4"
                            class="myMasterHead"
                            value="{searchModel>/merchantcode}"
                        />
                    </FlexBox>
                    <FlexBox
                        id="_IDGenFlexBox5"
                        direction="Column"
                        alignItems="Start"
                    >
                        <Label
                            id="_IDGenLabel4"
                            text="认领起始日期："
                            class="myMasterHead"
                        />
                        <DatePicker
                            id="_IDGenDatePicker"
                            class="myMasterHead"
                            value="{
                        path: 'searchModel>/claimsuccesstimestart',
                        type: 'sap.ui.model.type.Date',
                        formatOptions: {
                            style: 'short',
                            source: {pattern: 'yyyyMMdd'}
                        }}"
                        />
                    </FlexBox>
                    <FlexBox
                        id="_IDGenFlexBox6"
                        direction="Column"
                        alignItems="Start"
                    >
                        <Label
                            id="_IDGenLabel5"
                            text="认领截止日期："
                            class="myMasterHead"
                        />
                        <DatePicker
                            id="_IDGenDatePicker1"
                            class="myMasterHead"
                            value="{
                        path: 'searchModel>/claimsuccesstimeend',
                        type: 'sap.ui.model.type.Date',
                        formatOptions: {
                            style: 'short',
                            source: {pattern: 'yyyyMMdd'}
                        }}"
                        />
                    </FlexBox>
                </FlexBox>
                <FlexBox
                    id="_IDGenFlexBox7"
                    alignItems="Center"
                    justifyContent="End"
                >
                    <Button
                        id="_IDGenButton"
                        text="查询"
                        icon="sap-icon://search"
                        press=".onDataSearch"
                        type="Emphasized"
                        class="myMasterHeadButton"
                    />
                </FlexBox>
            </Panel>
        </content>
        <content>
            <table:Table
                id="_IDGenTable"
                rows="{dataModel>/data}"
                selectionMode="MultiToggle"
                paste="onPaste"
                ariaLabelledBy="title"
                class="myFontSize"
                sort="onSort"
                columnResize="onColumnResize"
                columnMove="onColumnMove"
                sap.ui.fl:flexibility="sap/m/flexibility/EngineFlex"
            >
                <!-- rowMode="Auto" -->
                <table:rowMode>
                    <r:Interactive
                        id="_IDGenFixed"
                        rowCount="12"
                    />
                </table:rowMode>
                <table:extension>
                    <OverflowToolbar
                        id="_IDGenOverflowToolbar"
                        style="Clear"
                    >
                        <Title
                            id="title"
                            text="CBS凭证清单"
                        />
                        <ToolbarSeparator id="_IDGenToolbarSeparator" />
                        <vm:VariantManagement
                            id="p13nVm"
                            for="_IDGenTable"
                        />
                        <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                        <Button
                            id="_IDGenButton2"
                            icon="sap-icon://save"
                            text="重处理"
                            press="onSave"
                        />
                        <Button
                            id="_IDGenButton5"
                            icon="sap-icon://excel-attachment"
                            text="导出"
                            press="onExport"
                        />
                        <Button
                            id="_IDGenButton4"
                            icon="sap-icon://action-settings"
                            text="设置"
                            press="openPersoDialog"
                        />
                    </OverflowToolbar>
                </table:extension>
                <table:dependents>
                    <columnmenu:Menu id="menu">
                        <columnmenu:items>
                            <columnmenu:ActionItem
                                id="_IDGenActionItem1"
                                icon="sap-icon://sort"
                                label="Sort"
                                press="onColumnHeaderItemPress"
                            />
                            <columnmenu:ActionItem
                                id="_IDGenActionItem2"
                                icon="sap-icon://table-column"
                                label="Columns"
                                press="onColumnHeaderItemPress"
                            />
                        </columnmenu:items>
                    </columnmenu:Menu>
                </table:dependents>
                <table:noData>
                    <IllustratedMessage
                        id="_IDGenIllustratedMessage"
                        enableVerticalResponsiveness="true"
                    />
                </table:noData>
                <table:rowSettingsTemplate>
                    <table:RowSettings
                        id="_IDGenRowSettings"
                        highlight="{dataModel>flagIcon}"
                        highlightText="{dataModel>msg}"
                    />
                </table:rowSettingsTemplate>
                <table:columns>
                    <table:Column
                        id="_IGenColumn"
                        width="4rem"
                        app:p13nKey="msg_col"
                        sortProperty="msg"
                        filterProperty="msg"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IGenLabel7"
                            text="消息"
                        />
                        <table:template>
                            <Text
                                id="_IGenText"
                                text="{dataModel>msg}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn22"
                        width="10rem"
                        app:p13nKey="transactionserialnumber_col"
                        sortProperty="transactionserialnumber"
                        filterProperty="transactionserialnumber"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel6"
                            text="交易流水号"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText2"
                                text="{dataModel>transactionserialnumber}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn23"
                        width="8rem"
                        app:p13nKey="accountingdocument_col"
                        sortProperty="accountingdocument"
                        filterProperty="accountingdocument"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel9"
                            text="会计凭证号"
                        />
                        <table:template>
                            <Link
                                id="_IDGenLink"
                                text="{dataModel>accountingdocument}"
                                href="{dataModel>accountingdocumenturl}"
                                target="_blank"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn"
                        width="8rem"
                        app:p13nKey="reversedaccountingdocument_col"
                        sortProperty="reversedaccountingdocument"
                        filterProperty="reversedaccountingdocument"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel7"
                            text="冲销会计凭证号"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText"
                                text="{dataModel>reversedaccountingdocument}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn1"
                        width="10rem"
                        app:p13nKey="extend1_col"
                        sortProperty="extend1"
                        filterProperty="extend1"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel8"
                            text="款项性质"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText1"
                                text="{dataModel>extend1}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>
                    <!-- <table:Column
                        id="_IDGenColumn2"
                        width="4rem"
                    >
                        <Label
                            id="_IDGenLabel9"
                            text="订单类型"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText2"
                                text="{dataModel>manufacturingordertype}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column> -->
                    <table:Column
                        id="_IDGenColumn3"
                        width="8rem"
                        app:p13nKey="paymentnaturedetail_col"
                        sortProperty="paymentnaturedetail"
                        filterProperty="paymentnaturedetail"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel10"
                            text="款项细分"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText3"
                                text="{dataModel>paymentnaturedetail}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn4"
                        width="8rem"
                        app:p13nKey="accountno_col"
                        sortProperty="accountno"
                        filterProperty="accountno"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel11"
                            text="银行账号"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText4"
                                text="{dataModel>accountno}"
                                wrapping="true"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn5"
                        width="8rem"
                        app:p13nKey="claimamount_col"
                        sortProperty="claimamount"
                        filterProperty="claimamount"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel12"
                            text="认领金额"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText5"
                                text="{dataModel>claimamount}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn6"
                        width="8rem"
                        app:p13nKey="claimsuccesstime_col"
                        sortProperty="claimsuccesstime"
                        filterProperty="claimsuccesstime"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel13"
                            text="认领日期"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText6"
                                text="{dataModel>claimsuccesstime}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn7"
                        width="8rem"
                        app:p13nKey="createbyname_col"
                        sortProperty="createbyname"
                        filterProperty="createbyname"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel14"
                            text="创建人姓名"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText7"
                                text="{dataModel>createbyname}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn8"
                        width="8rem"
                        app:p13nKey="banktransactiondate_col"
                        sortProperty="banktransactiondate"
                        filterProperty="banktransactiondate"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel15"
                            text="交易日期"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText8"
                                text="{dataModel>banktransactiondate}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn9"
                        width="8rem"
                        app:p13nKey="bankserialnumber_col"
                        sortProperty="bankserialnumber"
                        filterProperty="bankserialnumber"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel16"
                            text="银行流水号"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText9"
                                text="{dataModel>bankserialnumber}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn10"
                        width="10rem"
                        app:p13nKey="digest_col"
                        sortProperty="digest"
                        filterProperty="digest"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel17"
                            text="摘要"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText10"
                                text="{dataModel>digest}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn15"
                        width="6rem"
                        app:p13nKey="merchantcode_col"
                        sortProperty="merchantcode"
                        filterProperty="merchantcode"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel22"
                            text="客商编号"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText22"
                                text="{dataModel>merchantcode}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn16"
                        width="10rem"
                        app:p13nKey="merchantname_col"
                        sortProperty="merchantname"
                        filterProperty="merchantname"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel23"
                            text="客商名称"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText23"
                                text="{dataModel>merchantname}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn17"
                        width="10rem"
                        app:p13nKey="remark_col"
                        sortProperty="remark"
                        filterProperty="remark"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel24"
                            text="备注"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText24"
                                text="{dataModel>remark}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn11"
                        width="6rem"
                        app:p13nKey="companycode_col"
                        sortProperty="companycode"
                        filterProperty="companycode"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel18"
                            text="公司代码"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText11"
                                text="{dataModel>companycode}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn111"
                        width="8rem"
                        app:p13nKey="glaccount_col"
                        sortProperty="glaccount"
                        filterProperty="glaccount"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel181"
                            text="银行科目"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText111"
                                text="{dataModel>glaccount}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn12"
                        width="6rem"
                        app:p13nKey="housebank_col"
                        sortProperty="housebank"
                        filterProperty="housebank"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel19"
                            text="开户行"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText12"
                                text="{dataModel>housebank}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn13"
                        width="6rem"
                        app:p13nKey="housebankaccount_col"
                        sortProperty="housebankaccount"
                        filterProperty="housebankaccount"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel20"
                            text="开户行账户"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText13"
                                text="{dataModel>housebankaccount}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn14"
                        width="6rem"
                        app:p13nKey="transactioncurrency_col"
                        sortProperty="transactioncurrency"
                        filterProperty="transactioncurrency"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel21"
                            text="币种"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText14"
                                text="{dataModel>transactioncurrency}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        id="_IDGenColumn18"
                        width="8rem"
                        app:p13nKey="created_date_col"
                        sortProperty="created_date"
                        filterProperty="created_date"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel25"
                            text="接收日期"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText18"
                                text="{dataModel>created_date}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn19"
                        width="8rem"
                        app:p13nKey="updated_date_col"
                        sortProperty="updated_date"
                        filterProperty="updated_date"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel26"
                            text="处理日期"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText19"
                                text="{dataModel>updated_date}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn20"
                        width="8rem"
                        app:p13nKey="updated_by_col"
                        sortProperty="updated_by"
                        filterProperty="updated_by"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel27"
                            text="处理人"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText20"
                                text="{dataModel>updated_by}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                </table:columns>
            </table:Table>
        </content>
    </Page>
</mvc:View>
