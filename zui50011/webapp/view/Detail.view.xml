<mvc:View
    displayBlock="true"
    controllerName="zui50011.controller.Detail"
    height="100%"
    xmlns="sap.uxap"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:m="sap.m"
    xmlns:f="sap.f"
    xmlns:form="sap.ui.layout.form"
>
    <ObjectPageLayout
        id="ObjectPageLayout"
        showTitleInHeaderContent="true"
        alwaysShowContentHeader="false"
        preserveHeaderStateOnScroll="false"
        headerContentPinnable="true"
        isChildPage="true"
        upperCaseAnchorBar="false"
    >
        <headerTitle>
            <ObjectPageDynamicHeaderTitle
                id="_IDGenObjectPageDynamicHeaderTitle"
            >
                <expandedHeading>
                    <m:Title
                        id="_IDGenTitle"
                        text="{dataModel>outbillno}"
                        wrapping="true"
                        class="sapUiSmallMarginEnd"
                    />
                </expandedHeading>

                <snappedHeading>
                    <m:FlexBox
                        id="_IDGenFlexBox"
                        wrap="Wrap"
                        fitContainer="true"
                        alignItems="Center"
                    >
                        <m:FlexBox
                            id="_IDGenFlexBox1"
                            wrap="NoWrap"
                            fitContainer="true"
                            alignItems="Center"
                            class="sapUiTinyMarginEnd"
                        >
                            <!-- <m:Avatar src="../../../../../../../{dataModel>ProductPicUrl}" displaySize="S" displayShape="Square"/> -->
                            <m:Title
                                id="_IDGenTitle1"
                                text="{dataModel>outbillno}"
                                wrapping="true"
                                class="sapUiTinyMarginEnd"
                            />
                        </m:FlexBox>
                    </m:FlexBox>
                </snappedHeading>

                <navigationActions>
                    <m:OverflowToolbarButton
                        type="Transparent"
                        icon="sap-icon://full-screen"
                        press="handleFullScreen"
                        id="enterFullScreenBtn"
                        tooltip="Enter Full Screen Mode"
                        visible="{= ${/actionButtonsInfo/midColumn/fullScreen} !== null }"
                    />
                    <m:OverflowToolbarButton
                        type="Transparent"
                        icon="sap-icon://exit-full-screen"
                        press="handleExitFullScreen"
                        id="exitFullScreenBtn"
                        tooltip="Exit Full Screen Mode"
                        visible="{= ${/actionButtonsInfo/midColumn/exitFullScreen} !== null }"
                    />
                    <m:OverflowToolbarButton
                        id="_IDGenOverflowToolbarButton"
                        type="Transparent"
                        icon="sap-icon://decline"
                        press="handleClose"
                        tooltip="Close middle column"
                        visible="{= ${/actionButtonsInfo/midColumn/closeColumn} !== null }"
                    />
                </navigationActions>

                <!-- <actions>
							<m:Button
									text="Edit"
									type="Emphasized"
									press="toggleAreaPriority"/>
							<m:Button
									text="Delete"
									type="Transparent"/>
							<m:Button
									text="Copy"
									type="Transparent"/>
							<m:Button
									text="Toggle Footer"
									type="Transparent"
									press="onToggleFooter"/>
							<m:Button
									icon="sap-icon://action"
									type="Transparent"/>
						</actions> -->
            </ObjectPageDynamicHeaderTitle>
        </headerTitle>

        <headerContent>
            <m:FlexBox
                id="_IDGenFlexBox2"
                wrap="Wrap"
                fitContainer="true"
                alignItems="Stretch"
            >
                <!-- <m:Avatar src="../../../../../../../{dataModel>ProductPicUrl}"
								  displaySize="L"
								  displayShape="Square"
								  class="sapUiTinyMarginEnd">
						</m:Avatar> -->
                <m:VBox
                    id="_IDGenVBox11"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel11"
                        text="SAP订单号"
                    />
                    <m:Text
                        id="_IDGenText51"
                        text="{dataModel>purchaseorder}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox1"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel"
                        text="公司代码"
                    />
                    <m:Text
                        id="_IDGenText5"
                        text="{dataModel>companycode}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox2"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel1"
                        text="订单类型"
                    />
                    <m:Text
                        id="_IDGenText6"
                        text="{dataModel>purchaseordertype}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox3"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel2"
                        text="凭证日期"
                    />
                    <m:Text
                        id="_IDGenText7"
                        text="{dataModel>purchaseorderdate}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox4"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel3"
                        text="供应商"
                    />
                    <m:Text
                        id="_IDGenText8"
                        text="{dataModel>supplier}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox5"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel6"
                        text="采购组织"
                    />
                    <m:Text
                        id="_IDGenText9"
                        text="{dataModel>purchasingorganization}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox6"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel7"
                        text="采购组"
                    />
                    <m:Text
                        id="_IDGenText10"
                        text="{dataModel>purchasinggroup}"
                    />
                </m:VBox>
            </m:FlexBox>
        </headerContent>

        <sections>
            <ObjectPageSection
                id="_IDGenObjectPageSection"
                title="行项目"
            >
                <subSections>
                    <ObjectPageSubSection id="_IDGenObjectPageSubSection">
                        <blocks>
                            <m:Table
                                id="sssssTable"
                                items="{path : 'detailModel>/item'}"
                                autoPopinMode="true"
                                contextualWidth="Auto"
                            >
                                <m:columns>
                                    <m:Column
                                        width="5rem"
                                        importance="High"
                                        id="_IDGenColumn5"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText11"
                                            text="外围订单行号"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="5rem"
                                        importance="High"
                                        id="_IDGenColumn6"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText12"
                                            text="SAP订单行号"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="8rem"
                                        importance="High"
                                        id="_IDGenColumn7"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText13"
                                            text="工厂"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="8rem"
                                        importance="High"
                                        id="_IDGenColumn8"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText14"
                                            text="含税单价"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="8rem"
                                        importance="High"
                                        id="_IDGenColumn9"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText15"
                                            text="数量"
                                        />
                                    </m:Column>

                                    <m:Column
                                        width="6rem"
                                        importance="High"
                                        id="_IDGenColumn10"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText16"
                                            text="数量单位"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="5rem"
                                        importance="High"
                                        id="_IDGenColumn11"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText17"
                                            text="行类别"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="High"
                                        id="_IDGenColumn12"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText18"
                                            text="物料"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="Low"
                                        id="_IDGenColumn19"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText233"
                                            text="项目文本"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="High"
                                        id="_IDGenColumn13"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText19"
                                            text="税码"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="Low"
                                        id="_IDGenColumn14"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText20"
                                            text="交货时间"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="8rem"
                                        importance="Low"
                                        id="_IDGenColumn15"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText21"
                                            text="资产编号"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="Low"
                                        id="_IDGenColumn16"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText22"
                                            text="委外加工供应商"
                                        />
                                    </m:Column>

                                    <m:Column
                                        width="6rem"
                                        importance="Low"
                                        id="_IDGenColumn17"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText23"
                                            text="SC供应"
                                        />
                                    </m:Column>                                  
                                </m:columns>
                                <m:items>
                                    <m:ColumnListItem
                                        id="_IDGenColumnListItem1"
                                    >
                                        <m:cells>
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier3"
                                                text="{detailModel>outbillitemno}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier4"
                                                text="{detailModel>purchaseorderitem}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier5"
                                                text="{detailModel>plant}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier6"
                                                text="{detailModel>netpriceamount}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier7"
                                                text="{detailModel>orderquantity}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier8"
                                                text="{detailModel>purchaseorderquantityunit}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier9"
                                                text="{detailModel>purchaseorderitemcategory}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier10"
                                                text="{detailModel>material}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier11"
                                                text="{detailModel>purchaseorderitemtext}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier12"
                                                text="{detailModel>taxcode}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier13"
                                                text="{detailModel>schedulelinedeliverydate}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier14"
                                                text="{detailModel>masterfixedasset}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier15"
                                                text="{detailModel>subcontractor}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier16"
                                                text="{detailModel>supplierissubcontractor}"
                                            />
                                        </m:cells>
                                    </m:ColumnListItem>
                                </m:items>
                            </m:Table>
                        </blocks>
                    </ObjectPageSubSection>
                </subSections>
            </ObjectPageSection>
        </sections>
    </ObjectPageLayout>
</mvc:View>
