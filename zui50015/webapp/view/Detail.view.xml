<mvc:View
    displayBlock="true"
    controllerName="zui50015.controller.Detail"
    height="100%"
    xmlns="sap.uxap"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:m="sap.m"
    xmlns:f="sap.f"
    xmlns:form="sap.ui.layout.form"
>
    <ObjectPageLayout
        id="ObjectPageLayout"
        showTitleInHeaderContent="true"
        alwaysShowContentHeader="false"
        preserveHeaderStateOnScroll="false"
        headerContentPinnable="true"
        isChildPage="true"
        upperCaseAnchorBar="false"
    >
        <headerTitle>
            <ObjectPageDynamicHeaderTitle
                id="_IDGenObjectPageDynamicHeaderTitle"
            >
                <expandedHeading>
                    <m:Title
                        id="_IDGenTitle"
                        text="{dataModel>outbillno}"
                        wrapping="true"
                        class="sapUiSmallMarginEnd"
                    />
                </expandedHeading>

                <snappedHeading>
                    <m:FlexBox
                        id="_IDGenFlexBox"
                        wrap="Wrap"
                        fitContainer="true"
                        alignItems="Center"
                    >
                        <m:FlexBox
                            id="_IDGenFlexBox1"
                            wrap="NoWrap"
                            fitContainer="true"
                            alignItems="Center"
                            class="sapUiTinyMarginEnd"
                        >
                            <!-- <m:Avatar src="../../../../../../../{dataModel>ProductPicUrl}" displaySize="S" displayShape="Square"/> -->
                            <m:Title
                                id="_IDGenTitle1"
                                text="{dataModel>outbillno}"
                                wrapping="true"
                                class="sapUiTinyMarginEnd"
                            />
                        </m:FlexBox>
                    </m:FlexBox>
                </snappedHeading>

                <navigationActions>
                    <m:OverflowToolbarButton
                        type="Transparent"
                        icon="sap-icon://full-screen"
                        press="handleFullScreen"
                        id="enterFullScreenBtn"
                        tooltip="Enter Full Screen Mode"
                        visible="{= ${/actionButtonsInfo/midColumn/fullScreen} !== null }"
                    />
                    <m:OverflowToolbarButton
                        type="Transparent"
                        icon="sap-icon://exit-full-screen"
                        press="handleExitFullScreen"
                        id="exitFullScreenBtn"
                        tooltip="Exit Full Screen Mode"
                        visible="{= ${/actionButtonsInfo/midColumn/exitFullScreen} !== null }"
                    />
                    <m:OverflowToolbarButton
                        id="_IDGenOverflowToolbarButton"
                        type="Transparent"
                        icon="sap-icon://decline"
                        press="handleClose"
                        tooltip="Close middle column"
                        visible="{= ${/actionButtonsInfo/midColumn/closeColumn} !== null }"
                    />
                </navigationActions>

                <!-- <actions>
							<m:Button
									text="Edit"
									type="Emphasized"
									press="toggleAreaPriority"/>
							<m:Button
									text="Delete"
									type="Transparent"/>
							<m:Button
									text="Copy"
									type="Transparent"/>
							<m:Button
									text="Toggle Footer"
									type="Transparent"
									press="onToggleFooter"/>
							<m:Button
									icon="sap-icon://action"
									type="Transparent"/>
						</actions> -->
            </ObjectPageDynamicHeaderTitle>
        </headerTitle>

        <headerContent>
            <m:FlexBox
                id="_IDGenFlexBox2"
                wrap="Wrap"
                fitContainer="true"
                alignItems="Stretch"
            >
                <!-- <m:Avatar src="../../../../../../../{dataModel>ProductPicUrl}"
								  displaySize="L"
								  displayShape="Square"
								  class="sapUiTinyMarginEnd">
						</m:Avatar> -->
                <m:VBox
                    id="_IDGenVBox11"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel11"
                        text="SAP物料凭证号"
                    />
                    <m:Text
                        id="_IDGenText51"
                        text="{dataModel>materialdocument}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox1"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel"
                        text="凭证日期"
                    />
                    <m:Text
                        id="_IDGenText5"
                        text="{dataModel>documentdate}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox2"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel1"
                        text="过账日期"
                    />
                    <m:Text
                        id="_IDGenText6"
                        text="{dataModel>postingdate}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox3"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel2"
                        text="工厂"
                    />
                    <m:Text
                        id="_IDGenText7"
                        text="{dataModel>plant}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox4"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel3"
                        text="移动类型"
                    />
                    <m:Text
                        id="_IDGenText8"
                        text="{dataModel>goodsmovementtype}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox5"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel6"
                        text="导入日期"
                    />
                    <m:Text
                        id="_IDGenText9"
                        text="{dataModel>created_date}"
                    />
                </m:VBox>
                <m:VBox
                    id="_IDGenVBox6"
                    justifyContent="Center"
                    class="sapUiSmallMarginEnd"
                >
                    <m:Label
                        id="_IDGenLabel7"
                        text="导入用户"
                    />
                    <m:Text
                        id="_IDGenText10"
                        text="{dataModel>created_by}"
                    />
                </m:VBox>
            </m:FlexBox>
        </headerContent>

        <sections>
            <ObjectPageSection
                id="_IDGenObjectPageSection"
                title="行项目"
            >
                <subSections>
                    <ObjectPageSubSection id="_IDGenObjectPageSubSection">
                        <blocks>
                            <m:Table
                                id="sssssTable"
                                items="{path : 'detailModel>/item'}"
                                autoPopinMode="true"
                                contextualWidth="Auto"
                            >
                                <m:columns>
                                    <m:Column
                                        width="5rem"
                                        importance="High"
                                        id="_IDGenColumn5"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText11"
                                            text="导入凭证行号"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="8rem"
                                        importance="High"
                                        id="_IDGenColumn6"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText12"
                                            text="SAP物料凭证行号"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="High"
                                        id="_IDGenColumn7"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText13"
                                            text="移动类型"
                                        />
                                    </m:Column>
                                    <m:Column
                                        id="_IDGenColumn2"
                                        width="8rem"
                                        importance="High"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText3"
                                            text="特殊库存"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="8rem"
                                        importance="High"
                                        id="_IDGenColumn8"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText14"
                                            text="客户"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="8rem"
                                        importance="High"
                                        id="_IDGenColumn81"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText145"
                                            text="供应商"
                                        />
                                    </m:Column>
                                    <m:Column
                                        id="_IDGenColumn3"
                                        width="8rem"
                                        importance="High"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText4"
                                            text="成本中心"
                                        />
                                    </m:Column>
                                    <m:Column
                                        id="_IDGenColumn4"
                                        width="8rem"
                                        importance="High"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText24"
                                            text="WBS元素"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="8rem"
                                        importance="High"
                                        id="_IDGenColumn9"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText15"
                                            text="采购订单"
                                        />
                                    </m:Column>

                                    <m:Column
                                        width="6rem"
                                        importance="High"
                                        id="_IDGenColumn10"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText16"
                                            text="采购订单行"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="High"
                                        id="_IDGenColumn11"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText17"
                                            text="生产订单"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="High"
                                        id="_IDGenColumn12"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText18"
                                            text="物料"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="Low"
                                        id="_IDGenColumn19"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText233"
                                            text="工厂"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="High"
                                        id="_IDGenColumn13"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText19"
                                            text="库位"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="Low"
                                        id="_IDGenColumn14"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText20"
                                            text="WMS批次"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="8rem"
                                        importance="Low"
                                        id="_IDGenColumn15"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText21"
                                            text="生产日期"
                                        />
                                    </m:Column>
                                    <m:Column
                                        width="6rem"
                                        importance="Low"
                                        id="_IDGenColumn16"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText22"
                                            text="录入单位"
                                        />
                                    </m:Column>

                                    <m:Column
                                        width="6rem"
                                        importance="Low"
                                        id="_IDGenColumn17"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText23"
                                            text="录入数量"
                                        />
                                    </m:Column>
                                    <m:Column
                                        id="_IDGenColumn"
                                        width="6rem"
                                        importance="Low"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText1"
                                            text="传输库位"
                                        />
                                    </m:Column>
                                    <m:Column
                                        id="_IDGenColumn1"
                                        width="6rem"
                                        importance="Low"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText2"
                                            text="版本"
                                        />
                                    </m:Column>
                                    <m:Column
                                        id="_IDGenColumn122"
                                        width="6rem"
                                        importance="Low"
                                        hAlign="Center"
                                    >
                                        <m:Text
                                            id="_IDGenText222"
                                            text="最后一次收货"
                                        />
                                    </m:Column>
                                </m:columns>
                                <m:items>
                                    <m:ColumnListItem
                                        id="_IDGenColumnListItem1"
                                    >
                                        <m:cells>
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier3"
                                                text="{detailModel>outbillitemno}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier4"
                                                text="{detailModel>materialdocumentitem}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier5"
                                                text="{detailModel>goodsmovementtype}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier6"
                                                text="{detailModel>inventoryspecialstocktype}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier7"
                                                text="{detailModel>customer}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier71"
                                                text="{detailModel>supplier}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier711"
                                                text="{detailModel>costcenter}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier712"
                                                text="{detailModel>wbselement}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier8"
                                                text="{detailModel>purchaseorder}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier9"
                                                text="{detailModel>purchaseorderitem}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier10"
                                                text="{detailModel>manufacturingorder}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier11"
                                                text="{detailModel>material}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier12"
                                                text="{detailModel>plant}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier13"
                                                text="{detailModel>storagelocation}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier14"
                                                text="{detailModel>batchwms}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier15"
                                                text="{detailModel>manufacturedate}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier16"
                                                text="{detailModel>entryunit}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier1"
                                                text="{detailModel>quantityinentryunit}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier2"
                                                text="{detailModel>issuingorreceivingstorageloc}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier17"
                                                text="{detailModel>zvension}"
                                            />
                                            <m:ObjectIdentifier
                                                id="_IDGenObjectIdentifier171"
                                                text="{detailModel>zzlast}"
                                            />
                                        </m:cells>
                                    </m:ColumnListItem>
                                </m:items>
                            </m:Table>
                        </blocks>
                    </ObjectPageSubSection>
                </subSections>
            </ObjectPageSection>
        </sections>
    </ObjectPageLayout>
</mvc:View>
