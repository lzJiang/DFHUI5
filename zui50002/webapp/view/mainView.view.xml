<mvc:View
    controllerName="zui50002.controller.mainView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:columnmenu="sap.m.table.columnmenu"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:table="sap.ui.table"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns:sap.ui.fl="sap.ui.fl"
    xmlns:sap.ui.dt="sap.ui.dt"
    xmlns:vm="sap.ui.fl.variants"
    xmlns:r="sap.ui.table.rowmodes"
>
    <Page
        id="page"
        showHeader="false"
    >
        <content>
            <f:Form
                id="_IDGenForm1"
                editable="true"
            >
                <f:layout>
                    <f:ResponsiveGridLayout
                        id="_IDGenResponsiveGridLayout"
                        labelSpanL="1"
                        labelSpanM="1"
                        emptySpanL="0"
                        emptySpanM="0"
                        columnsL="1"
                        columnsM="1"
                    />
                </f:layout>
                <f:formContainers>
                    <f:FormContainer id="_IDGenFormContainer">
                        <f:formElements>
                            <f:FormElement id="_IDGenFormElement">
                                <f:fields>
                                    <l:HorizontalLayout
                                        id="_IDGenHorizontalLayout"
                                    >
                                        <Label
                                            id="_IDGenLabel"
                                            labelFor='ids1'
                                            text="工厂："
                                            class="myMasterHead"
                                        />
                                        <Select
                                            id="_IDGenSelect"
                                            width="15rem"
                                            forceSelection="true"
                                            selectedKey="{searchModel>/productionplant}"
                                        >
                                            <core:Item
                                                id="_IDGenItem2"
                                                key="1100"
                                                text="北京东方红航天生物技术工厂"
                                            />
                                            <core:Item
                                                id="_IDGenItem3"
                                                key="4100"
                                                text="河北天辰阳光科技工厂"
                                            />
                                        </Select>
                                        <Label
                                            id="_IDGenLabel2"
                                            labelFor='ids3'
                                            text="计划订单："
                                            class="myMasterHead"
                                        />
                                        <Input
                                            id='ids3'
                                            width="10rem"
                                            value="{searchModel>/plannedorder}"
                                        />
                                        <Label
                                            id="_IDGenLabel3"
                                            labelFor='ids4'
                                            text="计划批次："
                                            class="myMasterHead"
                                        />
                                        <Input
                                            id='ids4'
                                            width="10rem"
                                            value="{searchModel>/yy1_plannebatch_pla}"
                                        />
                                        <Label
                                            id="_IDGenLabel4"
                                            labelFor='ids5'
                                            text="开始日期："
                                            class="myMasterHead"
                                        />
                                        <DatePicker
                                            id='ids5'
                                            width="8rem"
                                            value="{
											path: 'searchModel>/plndorderplannedstartdate',
											type: 'sap.ui.model.type.Date',
											formatOptions: {
                                                style: 'short',
												source: {pattern: 'yyyyMMdd'}
											}}"
                                        />
                                        <Label
                                            id="_IDGenLabel5"
                                            labelFor='ids6'
                                            text="结束日期："
                                            class="myMasterHead"
                                        />
                                        <DatePicker
                                            id='ids6'
                                            width="8rem"
                                            value="{
											path: 'searchModel>/plndorderplannedenddate',
											type: 'sap.ui.model.type.Date',
											formatOptions: {
                                                style: 'short',
												source: {pattern: 'yyyyMMdd'}
											}}"
                                        />
                                    </l:HorizontalLayout>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement id="_IDGenFormElement1">
                                <f:fields>
                                    <l:HorizontalLayout
                                        id="_IDGenHorizontalLayout1"
                                    >
                                        <Label
                                            id="_IDGenLabel33"
                                            labelFor='ids33'
                                            text="MRP控制者："
                                            class="myMasterHead"
                                        />
                                        <Input
                                            id='ids33'
                                            width="15rem"
                                            value="{searchModel>/mrpcontroller}"
                                        />
                                        <Label
                                            id="_IDGenLabel1"
                                            labelFor='ids2'
                                            text="物料："
                                            class="myMasterHead"
                                        />
                                        <Input
                                            id='ids2'
                                            width="10rem"
                                            value="{searchModel>/material}"
                                        />
                                        <Label
                                            id="_IDGenLabel34"
                                            labelFor='ids34'
                                            text="物料名称："
                                            class="myMasterHead"
                                        />
                                        <Input
                                            id='ids34'
                                            width="10rem"
                                            value="{searchModel>/productname}"
                                        />
                                        <Button
                                            id="_IDGenButton"
                                            text="查询"
                                            icon="sap-icon://search"
                                            press=".onDataSearch"
                                            type="Accept"
                                            class="myMasterHead"
                                        />
                                    </l:HorizontalLayout>
                                </f:fields>
                            </f:FormElement>
                        </f:formElements>
                    </f:FormContainer>
                </f:formContainers>
            </f:Form>
        </content>
        <content>
            <table:Table
                id="_IDGenTable"
                rows="{dataModel>/data}"
                selectionMode="MultiToggle"
                paste="onPaste"
                ariaLabelledBy="title"
                class="myFontSize"
                sort="onSort"
                columnResize="onColumnResize"
                columnMove="onColumnMove"
                sap.ui.fl:flexibility="sap/m/flexibility/EngineFlex"
            >
                <table:rowMode>
                    <r:Interactive
                        id="_IDGenFixed"
                        rowCount="12"
                    />
                </table:rowMode>
                <table:extension>
                    <OverflowToolbar
                        id="_IDGenOverflowToolbar"
                        style="Clear"
                    >
                        <Title
                            id="title"
                            text="{appModel>/currentType}"
                        />
                        <ToolbarSeparator id="_IDGenToolbarSeparator" />
                        <vm:VariantManagement
                            id="p13nVm"
                            for="_IDGenTable"
                        />
                        <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                        <Button
                            id="_IDGenButton1"
                            icon="sap-icon://edit"
                            text="更改"
                            press="onEdit"
                        />
                        <Button
                            id="_IDGenButton2"
                            icon="sap-icon://save"
                            text="保存"
                            press="onSave"
                            enabled="{appModel>/enableSave}"
                        />
                        <Button
                            id="_IDGenButton5"
                            icon="sap-icon://excel-attachment"
                            text="导出"
                            press="onExport"
                        />
                        <Button
                            id="_IDGenButton4"
                            icon="sap-icon://action-settings"
                            text="设置"
                            press="openPersoDialog"
                        />
                    </OverflowToolbar>
                </table:extension>
                <table:dependents>
                    <columnmenu:Menu id="menu">
                        <columnmenu:items>
                            <columnmenu:ActionItem
                                id="_IDGenActionItem1"
                                icon="sap-icon://sort"
                                label="Sort"
                                press="onColumnHeaderItemPress"
                            />
                            <columnmenu:ActionItem
                                id="_IDGenActionItem2"
                                icon="sap-icon://table-column"
                                label="Columns"
                                press="onColumnHeaderItemPress"
                            />
                        </columnmenu:items>
                    </columnmenu:Menu>
                </table:dependents>
                <table:noData>
                    <IllustratedMessage
                        id="_IDGenIllustratedMessage"
                        enableVerticalResponsiveness="true"
                    />
                </table:noData>
                <table:rowSettingsTemplate>
                    <table:RowSettings
                        id="_IDGenRowSettings"
                        highlight="{dataModel>flagIcon}"
                        highlightText="{dataModel>yy1_msg}"
                    />
                </table:rowSettingsTemplate>
                <table:columns>
                    <table:Column
                        id="_IGenColumn"
                        width="4rem"
                        app:p13nKey="yy1_msg_col"
                        sortProperty="yy1_msg"
                        filterProperty="yy1_msg"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IGenLabel7"
                            text="消息"
                        />
                        <table:template>
                            <Text
                                id="_IGenText"
                                text="{dataModel>yy1_msg}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn"
                        width="4rem"
                        app:p13nKey="productionplant_col"
                        sortProperty="productionplant"
                        filterProperty="productionplant"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel7"
                            text="工厂"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText"
                                text="{dataModel>productionplant}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn1"
                        width="6rem"
                        app:p13nKey="plannedorder_col"
                        sortProperty="plannedorder"
                        filterProperty="plannedorder"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel8"
                            text="订单"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText1"
                                text="{dataModel>plannedorder}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn5"
                        width="6rem"
                        app:p13nKey="material_col"
                        sortProperty="material"
                        filterProperty="material"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel12"
                            text="物料"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText5"
                                text="{dataModel>material}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn6"
                        width="8rem"
                        app:p13nKey="productname_col"
                        sortProperty="productname"
                        filterProperty="productname"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel13"
                            text="物料描述"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText6"
                                text="{dataModel>productname}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn7"
                        width="6rem"
                        app:p13nKey="totalquantity_col"
                        sortProperty="totalquantity"
                        filterProperty="totalquantity"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel14"
                            text="计划数量"
                        />
                        <table:template>
                            <Input
                                id="_IDGenText7"
                                value="{dataModel>totalquantity}"
                                editable="{dataModel>editable}"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn8"
                        width="4rem"
                        app:p13nKey="productionunit_col"
                        sortProperty="productionunit"
                        filterProperty="productionunit"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel15"
                            text="单位"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText8"
                                text="{dataModel>productionunit}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn15"
                        width="8rem"
                        app:p13nKey="yy1_plannebatch_pla_col"
                        sortProperty="yy1_plannebatch_pla"
                        filterProperty="yy1_plannebatch_pla"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel22"
                            text="计划批次"
                        />
                        <table:template>
                            <Input
                                id="_IDGenText15"
                                value="{dataModel>yy1_plannebatch_pla}"
                                editable="{dataModel>editable}"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn9"
                        width="10rem"
                        app:p13nKey="plndorderplannedstartdate_col"
                        sortProperty="plndorderplannedstartdate"
                        filterProperty="plndorderplannedstartdate"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel16"
                            text="计划开始日期"
                        />
                        <table:template>
                            <DatePicker
                                id='_IDGenText9'
                                value="{    path: 'dataModel>plndorderplannedstartdate',
											type: 'sap.ui.model.type.Date',
											formatOptions: {
                                                style: 'short',
												source: {pattern: 'yyyy-MM-dd'}
											}}"
                                editable="{dataModel>editable}"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn10"
                        width="10rem"
                        app:p13nKey="plndorderplannedenddate_col"
                        sortProperty="plndorderplannedenddate"
                        filterProperty="plndorderplannedenddate"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel17"
                            text="计划结束日期"
                        />
                        <table:template>
                            <DatePicker
                                id='_IDGenText10'
                                value="{    path: 'dataModel>plndorderplannedenddate',
											type: 'sap.ui.model.type.Date',
											formatOptions: {
                                                style: 'short',
												source: {pattern: 'yyyy-MM-dd'}
											}}"
                                editable="{dataModel>editable}"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        id="_IDGenColumn51"
                        width="6rem"
                        app:p13nKey="mrpcontroller_col"
                        sortProperty="mrpcontroller"
                        filterProperty="mrpcontroller"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel51"
                            text="MRP控制者"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText51"
                                text="{dataModel>mrpcontroller}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn52"
                        width="5rem"
                        app:p13nKey="mrpgroup_col"
                        sortProperty="mrpgroup"
                        filterProperty="mrpgroup"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel52"
                            text="MRP组"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText52"
                                text="{dataModel>mrpgroup}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn53"
                        width="5rem"
                        app:p13nKey="productionversion_col"
                        sortProperty="productionversion"
                        filterProperty="productionversion"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel53"
                            text="生产版本"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText53"
                                text="{dataModel>productionversion}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn54"
                        width="5rem"
                        app:p13nKey="plannedorderisfirm_col"
                        sortProperty="plannedorderisfirm"
                        filterProperty="plannedorderisfirm"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel54"
                            text="固定标识"
                        />
                        <table:template>
                            <CheckBox
                                id="_IDGenCheckBox"
                                selected="{dataModel>plannedorderisfirm_bool}"
                                editable="{dataModel>editable}"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn55"
                        width="6rem"
                        app:p13nKey="lastchangedbyuser"
                        sortProperty="lastchangedbyuser"
                        filterProperty="lastchangedbyuser"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel55"
                            text="更改人"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText55"
                                text="{dataModel>lastchangedbyuser}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn56"
                        width="6rem"
                        app:p13nKey="lastchangedate_col"
                        sortProperty="lastchangedate"
                        filterProperty="lastchangedate"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel56"
                            text="更改日期"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText56"
                                text="{dataModel>lastchangedate}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                </table:columns>
            </table:Table>
        </content>
    </Page>
</mvc:View>
