<mvc:View
    controllerName="zui50008.controller.mainView"
    xmlns:l="sap.ui.layout"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <l:VerticalLayout
        id="_IDGenVerticalLayout1"
        class="sapUiContentPadding myCenter"
    >
        <l:HorizontalLayout
            id="_IDGenHorizontalLayout"
            class="myTop"
        >
            <Label
                id="_IDGenLabel34"
                labelFor='ids34'
                text="工厂："
            />
            <Select
                id="ids34"
                width="16rem"
                forceSelection="false"
                selectedKey="{dataModel>/productionplant}"
            >
                <core:Item
                    id="_IDGenItem2"
                    key="1100"
                    text="北京东方红航天生物技术工厂"
                />
                <core:Item
                    id="_IDGenItem3"
                    key="4100"
                    text="河北天辰阳光科技工厂"
                />
            </Select>
        </l:HorizontalLayout>

        <Button
            id="_IDGenButton1"
            class="myButton"
            icon="sap-icon://menu"
            text="创建生产领料单"
            press="onScll"
        />
        <Button
            id="_IDGenButton2"
            class="myButton"
            icon="sap-icon://menu"
            text="创建委外领料单"
            press="onWwll"
        />
        <Button
            id="_IDGenButton3"
            class="myButton"
            icon="sap-icon://menu"
            text="创建其他领料单"
            press="onQtll"
        />
        <Button
            id="_IDGenButton4"
            class="myButton"
            icon="sap-icon://menu"
            text="创建其他退料单"
            press="onQttl"
        />
        <Button
            id="_IDGenButton5"
            class="myButton"
            icon="sap-icon://menu"
            text="领料单管理"
            press="onLlgl"
        />
    </l:VerticalLayout>
</mvc:View>
