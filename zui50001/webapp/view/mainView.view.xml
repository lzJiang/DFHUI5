<mvc:View
    controllerName="zui50001.controller.mainView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:columnmenu="sap.m.table.columnmenu"
    xmlns:core="sap.ui.core"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:table="sap.ui.table"
    xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns:sap.ui.fl="sap.ui.fl"
    xmlns:sap.ui.dt="sap.ui.dt"
    xmlns:vm="sap.ui.fl.variants"
>
    <Page
        id="page"
        showHeader="false"
    >
        <content>
            <f:Form
                id="_IDGenForm1"
                editable="true"
            >
                <f:layout>
                    <f:ResponsiveGridLayout
                        id="_IDGenResponsiveGridLayout"
                        labelSpanL="1"
                        labelSpanM="1"
                        emptySpanL="0"
                        emptySpanM="0"
                        columnsL="1"
                        columnsM="1"
                    />
                </f:layout>
                <f:formContainers>
                    <f:FormContainer id="_IDGenFormContainer">
                        <f:formElements>
                            <f:FormElement id="_IDGenFormElement">
                                <f:fields>
                                    <l:HorizontalLayout
                                        id="_IDGenHorizontalLayout"
                                    >
                                        <Label
                                            id="_IDGenLabel"
                                            labelFor='ids1'
                                            text="工厂："
                                            class="myMasterHead"
                                        />
                                        <Select
                                            id="_IDGenSelect"
                                            width="15rem"
                                            forceSelection="true"
                                            selectedKey="{searchModel>/productionplant}"
                                        >
                                            <core:Item
                                                id="_IDGenItem2"
                                                key="1100"
                                                text="北京东方红航天生物技术工厂"
                                            />
                                            <core:Item
                                                id="_IDGenItem3"
                                                key="4100"
                                                text="河北天辰阳光科技工厂"
                                            />
                                        </Select>
                                        <Label
                                            id="_IDGenLabel2"
                                            labelFor='ids3'
                                            text="订单类型："
                                            class="myMasterHead"
                                        />
                                        <Select
                                            id="_IDGenSelectSearch2"
                                            width="10rem"
                                            forceSelection="false"
                                            selectedKey="{searchModel>/manufacturingordertype}"
                                        >
                                            <core:Item
                                                id="_IDGenItem28"
                                                key=""
                                                text=""
                                            />
                                            <core:Item
                                                id="_IDGenItem8"
                                                key="红曲车间"
                                                text="红曲车间"
                                            />
                                            <core:Item
                                                id="_IDGenItem9"
                                                key="固体制剂车间"
                                                text="固体制剂车间"
                                            />
                                            <core:Item
                                                id="_IDGenItem10"
                                                key="包装车间"
                                                text="包装车间"
                                            />
                                            <core:Item
                                                id="_IDGenItem11"
                                                key="软胶囊车间"
                                                text="软胶囊车间"
                                            />
                                            <core:Item
                                                id="_IDGenItem12"
                                                key="口服液车间"
                                                text="口服液车间"
                                            />
                                            <core:Item
                                                id="_IDGenItem13"
                                                key="研发试制订单"
                                                text="研发试制订单"
                                            />
                                        </Select>
                                        <Label
                                            id="_IDGenLabel3"
                                            labelFor='ids4'
                                            text="生产批次："
                                            class="myMasterHead"
                                        />
                                        <Input
                                            id='ids4'
                                            width="8rem"
                                            value="{searchModel>/yy1_mfgbatch_ord}"
                                        />
                                        <Label
                                            id="_IDGenLabel4"
                                            labelFor='ids5'
                                            text="开始日期："
                                            class="myMasterHead"
                                        />
                                        <DatePicker
                                            id='ids5'
                                            width="8rem"
                                            value="{
											path: 'searchModel>/mfgorderplannedstartdate',
											type: 'sap.ui.model.type.Date',
											formatOptions: {
                                                style: 'short',
												source: {pattern: 'yyyyMMdd'}
											}}"
                                        />
                                        <Label
                                            id="_IDGenLabel5"
                                            labelFor='ids6'
                                            text="结束日期："
                                            class="myMasterHead"
                                        />
                                        <DatePicker
                                            id='ids6'
                                            width="8rem"
                                            value="{
											path: 'searchModel>/mfgorderplannedenddate',
											type: 'sap.ui.model.type.Date',
											formatOptions: {
                                                style: 'short',
												source: {pattern: 'yyyyMMdd'}
											}}"
                                        />
                                        <Label
                                            id="_IDGenLabel6"
                                            width="1rem"
                                        />
                                    </l:HorizontalLayout>
                                </f:fields>
                            </f:FormElement>
                            <f:FormElement id="_IDGenFormElement33">
                                <f:fields>
                                    <l:HorizontalLayout
                                        id="_IDGenHorizontalLayout33"
                                    >
                                        <Label
                                            id="_IDGenLabel33"
                                            labelFor='ids33'
                                            text="订单："
                                            class="myMasterHead"
                                        />
                                        <Input
                                            id='ids33'
                                            width="15rem"
                                            value="{searchModel>/manufacturingorder}"
                                        />
                                        <Label
                                            id="_IDGenLabel1"
                                            labelFor='ids2'
                                            text="物料："
                                            class="myMasterHead"
                                        />
                                        <Input
                                            id='ids2'
                                            width="10rem"
                                            value="{searchModel>/material}"
                                        />
                                        <Label
                                            id="_IDGenLabel34"
                                            labelFor='ids34'
                                            text="物料名称："
                                            class="myMasterHead"
                                        />
                                        <Input
                                            id='ids34'
                                            width="8rem"
                                            value="{searchModel>/productname}"
                                        />
                                        <Button
                                            id="_IDGenButton"
                                            text="查询"
                                            icon="sap-icon://search"
                                            press=".onDataSearch"
                                            type="Accept"
                                            class="myMasterHeadButton"
                                        />
                                    </l:HorizontalLayout>
                                </f:fields>
                            </f:FormElement>
                        </f:formElements>
                    </f:FormContainer>
                </f:formContainers>
            </f:Form>
        </content>
        <content>
            <table:Table
                id="_IDGenTable"
                rows="{dataModel>/data}"
                rowMode="Auto"
                selectionMode="MultiToggle"
                paste="onPaste"
                ariaLabelledBy="title"
                class="myFontSize"
                sort="onSort"
                columnResize="onColumnResize"
                columnMove="onColumnMove"
                sap.ui.fl:flexibility="sap/m/flexibility/EngineFlex"
            >
                <table:extension>
                    <OverflowToolbar
                        id="_IDGenOverflowToolbar"
                        style="Clear"
                    >
                        <Title
                            id="title"
                            text="{appModel>/currentType}"
                        />
                        <ToolbarSeparator id="_IDGenToolbarSeparator" />
                        <vm:VariantManagement
                            id="p13nVm"
                            for="_IDGenTable"
                        />
                        <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                        <Button
                            id="_IDGenButton1"
                            icon="sap-icon://edit"
                            text="更改"
                            press="onEdit"
                        />
                        <Button
                            id="_IDGenButton2"
                            icon="sap-icon://save"
                            text="保存"
                            press="onSave"
                            enabled="{appModel>/enableSave}"
                        />
                        <Button
                            id="_IDGenButton11"
                            icon="sap-icon://shipping-status"
                            text="推送BPM审批"
                            press="onSendBpm"
                        />
                        <Button
                            id="_IDGenButton3"
                            icon="sap-icon://shipping-status"
                            text="下达"
                            press="onRelease"
                        />
                        <Button
                            id="_IDGenButton6"
                            icon="sap-icon://shipping-status"
                            text="手工推送WMS"
                            press="onSendWms"
                        />
                        <Button
                            id="_IDGenButton8"
                            icon="sap-icon://print"
                            text="打印生产通知单"
                            press="onPrint"
                        />
                        <Button
                            id="_IDGenButton5"
                            icon="sap-icon://excel-attachment"
                            text="导出"
                            press="onExport"
                        />
                        <Button
                            id="_IDGenButton4"
                            icon="sap-icon://action-settings"
                            text="设置"
                            press="openPersoDialog"
                        />
                    </OverflowToolbar>
                </table:extension>
                <table:dependents>
                    <columnmenu:Menu id="menu">
                        <columnmenu:items>
                            <columnmenu:ActionItem
                                id="_IDGenActionItem1"
                                icon="sap-icon://sort"
                                label="Sort"
                                press="onColumnHeaderItemPress"
                            />
                            <columnmenu:ActionItem
                                id="_IDGenActionItem2"
                                icon="sap-icon://table-column"
                                label="Columns"
                                press="onColumnHeaderItemPress"
                            />
                        </columnmenu:items>
                    </columnmenu:Menu>
                </table:dependents>
                <table:noData>
                    <IllustratedMessage
                        id="_IDGenIllustratedMessage"
                        enableVerticalResponsiveness="true"
                    />
                </table:noData>
                <table:rowSettingsTemplate>
                    <table:RowSettings
                        id="_IDGenRowSettings"
                        highlight="{dataModel>flagIcon}"
                        highlightText="{dataModel>yy1_msg}"
                    />
                </table:rowSettingsTemplate>
                <table:columns>
                    <table:Column
                        id="_IGenColumn"
                        width="4rem"
                        app:p13nKey="yy1_msg_col"
                        sortProperty="yy1_msg"
                        filterProperty="yy1_msg"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IGenLabel7"
                            text="消息"
                        />
                        <table:template>
                            <Text
                                id="_IGenText"
                                text="{dataModel>yy1_msg}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn"
                        width="4rem"
                        app:p13nKey="productionplant_col"
                        sortProperty="productionplant"
                        filterProperty="productionplant"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel7"
                            text="工厂"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText"
                                text="{dataModel>productionplant}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn1"
                        width="6rem"
                        app:p13nKey="manufacturingorder_col"
                        sortProperty="manufacturingorder"
                        filterProperty="manufacturingorder"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel8"
                            text="订单"
                        />
                        <table:template>
                            <!-- <Text
                                id="_IDGenText1"
                                text="{dataModel>manufacturingorder}"
                                wrapping="false"
                            /> -->
                            <Link id="_IDGenLink"
                                text="{dataModel>manufacturingorder}"
                                href="{dataModel>manufacturingorderurl}"
                                target="_blank"
                            />
                        </table:template>
                    </table:Column>
                    <!-- <table:Column
                        id="_IDGenColumn2"
                        width="4rem"
                    >
                        <Label
                            id="_IDGenLabel9"
                            text="订单类型"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText2"
                                text="{dataModel>manufacturingordertype}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column> -->
                    <table:Column
                        id="_IDGenColumn3"
                        width="6rem"
                        app:p13nKey="manufacturingordertypename_col"
                        sortProperty="manufacturingordertypename"
                        filterProperty="manufacturingordertypename"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel10"
                            text="订单类型"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText3"
                                text="{dataModel>manufacturingordertypename}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn4"
                        width="6rem"
                        app:p13nKey="status_col"
                        sortProperty="status"
                        filterProperty="status"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel11"
                            text="订单状态"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText4"
                                text="{dataModel>status}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn5"
                        width="6rem"
                        app:p13nKey="material_col"
                        sortProperty="material"
                        filterProperty="material"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel12"
                            text="物料"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText5"
                                text="{dataModel>material}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn6"
                        width="8rem"
                        app:p13nKey="productname_col"
                        sortProperty="productname"
                        filterProperty="productname"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel13"
                            text="物料描述"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText6"
                                text="{dataModel>productname}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn7"
                        width="6rem"
                        app:p13nKey="mfgorderplannedtotalqty_col"
                        sortProperty="mfgorderplannedtotalqty"
                        filterProperty="mfgorderplannedtotalqty"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel14"
                            text="订单数量"
                        />
                        <table:template>
                            <Input
                                id="_IDGenText7"
                                value="{dataModel>mfgorderplannedtotalqty}"
                                editable="{dataModel>editable}"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn8"
                        width="4rem"
                        app:p13nKey="productionunit_col"
                        sortProperty="productionunit"
                        filterProperty="productionunit"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel15"
                            text="单位"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText8"
                                text="{dataModel>productionunit}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn9"
                        width="10rem"
                        app:p13nKey="mfgorderplannedstartdate_col"
                        sortProperty="mfgorderplannedstartdate"
                        filterProperty="mfgorderplannedstartdate"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel16"
                            text="开始日期"
                        />
                        <table:template>
                            <DatePicker
                                id='_IDGenText9'
                                value="{    path: 'dataModel>mfgorderplannedstartdate',
											type: 'sap.ui.model.type.Date',
											formatOptions: {
                                                style: 'short',
												source: {pattern: 'yyyy-MM-dd'}
											}}"
                                editable="{dataModel>editable}"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn10"
                        width="10rem"
                        app:p13nKey="mfgorderplannedenddate_col"
                        sortProperty="mfgorderplannedenddate"
                        filterProperty="mfgorderplannedenddate"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel17"
                            text="结束日期"
                        />
                        <table:template>
                            <DatePicker
                                id='_IDGenText10'
                                value="{    path: 'dataModel>mfgorderplannedenddate',
											type: 'sap.ui.model.type.Date',
											formatOptions: {
                                                style: 'short',
												source: {pattern: 'yyyy-MM-dd'}
											}}"
                                editable="{dataModel>editable}"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn15"
                        width="8rem"
                        app:p13nKey="yy1_mfgbatch_ord_col"
                        sortProperty="yy1_mfgbatch_ord"
                        filterProperty="yy1_mfgbatch_ord"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel22"
                            text="生产批次"
                        />
                        <table:template>
                            <Input
                                id="_IDGenText15"
                                value="{dataModel>yy1_mfgbatch_ord}"
                                editable="{dataModel>editable}"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn16"
                        width="10rem"
                        app:p13nKey="yy1_mfgdate_col"
                        sortProperty="yy1_mfgdate"
                        filterProperty="yy1_mfgdate"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel23"
                            text="生产日期"
                        />
                        <table:template>
                            <DatePicker
                                id='_IDGenText16'
                                value="{    path: 'dataModel>yy1_mfgdate',
											type: 'sap.ui.model.type.Date',
											formatOptions: {
                                                style: 'short',
												source: {pattern: 'yyyy-MM-dd'}
											}}"
                                editable="{dataModel>editable}"
                                change=".onScrqChange"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn17"
                        width="10rem"
                        app:p13nKey="yy1_expdate_col"
                        sortProperty="yy1_expdate"
                        filterProperty="yy1_expdate"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel24"
                            text="有效期"
                        />
                        <table:template>
                            <DatePicker
                                id='_IDGenText17'
                                value="{    path: 'dataModel>yy1_expdate',
											type: 'sap.ui.model.type.Date',
											formatOptions: {
                                                style: 'short',
												source: {pattern: 'yyyy-MM-dd'}
											}}"
                                editable="{dataModel>editable}"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn11"
                        width="6rem"
                        app:p13nKey="mfgorderactualreleasedate_col"
                        sortProperty="mfgorderactualreleasedate"
                        filterProperty="mfgorderactualreleasedate"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel18"
                            text="下达日期"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText11"
                                text="{dataModel>mfgorderactualreleasedate}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn12"
                        width="4rem"
                        app:p13nKey="lastchangedbyuser_col"
                        sortProperty="lastchangedbyuser"
                        filterProperty="lastchangedbyuser"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel19"
                            text="更改人"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText12"
                                text="{dataModel>lastchangedbyuser}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn13"
                        width="6rem"
                        app:p13nKey="mfgordercreationdate_col"
                        sortProperty="mfgordercreationdate"
                        filterProperty="mfgordercreationdate"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel20"
                            text="创建日期"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText13"
                                text="{dataModel>mfgordercreationdate}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn14"
                        width="6rem"
                        app:p13nKey="yy1_plannebatch_ord_col"
                        sortProperty="yy1_plannebatch_ord"
                        filterProperty="yy1_plannebatch_ord"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel21"
                            text="计划批次"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText14"
                                text="{dataModel>yy1_plannebatch_ord}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>

                    <table:Column
                        id="_IDGenColumn18"
                        width="6rem"
                        app:p13nKey="yy1_approve_col"
                        sortProperty="yy1_approve"
                        filterProperty="yy1_approve"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel25"
                            text="批准人"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText18"
                                text="{dataModel>yy1_approve}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn19"
                        width="6rem"
                        app:p13nKey="yy1_approveTime_col"
                        sortProperty="yy1_approveTime"
                        filterProperty="yy1_approveTime"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel26"
                            text="批准时间"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText19"
                                text="{dataModel>yy1_approvetime}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn20"
                        width="6rem"
                        app:p13nKey="yy1_check_col"
                        sortProperty="yy1_check"
                        filterProperty="yy1_check"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel27"
                            text="审批人"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText20"
                                text="{dataModel>yy1_check}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                    <table:Column
                        id="_IDGenColumn21"
                        width="6rem"
                        app:p13nKey="yy1_checktime_col"
                        sortProperty="yy1_checktime"
                        filterProperty="yy1_checktime"
                        headerMenu="menu"
                        resizable="true"
                        autoResizable="true"
                    >
                        <Label
                            id="_IDGenLabel28"
                            text="审批时间"
                        />
                        <table:template>
                            <Text
                                id="_IDGenText21"
                                text="{dataModel>yy1_checktime}"
                                wrapping="false"
                            />
                        </table:template>
                    </table:Column>
                </table:columns>
            </table:Table>
        </content>
    </Page>
</mvc:View>
